@{
    ViewData["Title"] = "Home Page";
}
<br />


    <div class="row">
        <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
            <ol class="carousel-indicators">
                <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                <li data-target="#myCarousel" data-slide-to="1"></li>
                <li data-target="#myCarousel" data-slide-to="2"></li>
                <li data-target="#myCarousel" data-slide-to="3"></li>
                <li data-target="#myCarousel" data-slide-to="4"></li>
            </ol>
            <div class="carousel-inner" role="listbox">
                <div class="item active">
                    <img src="~/images/war_car.jpg" alt="ASP.NET" class="img-responsive" />
                    <div class="carousel-caption" role="option">
                        <p>
                            War Memorial
                        </p>
                    </div>
                </div>
                <div class="item">
                    <img src="~/images/arboretum_car.jpg" alt="Visual Studio" class="img-responsive" />
                    <div class="carousel-caption" role="option">
                        <p>
                            National Arboretum
                        </p>
                    </div>
                </div>
                <div class="item">
                    <img src="~/images/zoo_car.jpg" alt="Package Management" class="img-responsive" />
                    <div class="carousel-caption" role="option">
                        <p>
                            Canberra Zoo
                        </p>
                    </div>
                </div>
                <div class="item">
                    <img src="~/images/house_car.jpg" alt="Microsoft Azure" class="img-responsive" />
                    <div class="carousel-caption" role="option">
                        <p>
                            Parliament House
                        </p>
                    </div>
                </div>
                <div class="item">
                    <img src="~/images/tower_car.jpg" alt="Microsoft Azure" class="img-responsive" />
                    <div class="carousel-caption" role="option">
                        <p>
                            Telstra Tower
                        </p>
                    </div>
                </div>
            </div>
            <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
                <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
                <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
        <br />

        
        <ul id="myTab" class="nav nav-tabs">
            <li class="active">
                <a href="#all" data-toggle="tab">
                    Show All
                </a>
            </li>
            
            <li class="dropdown">
                <a href="#" id="myTabDrop1" class="dropdown-toggle"
